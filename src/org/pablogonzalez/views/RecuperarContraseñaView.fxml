<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.pablogonzalez.controllers.RecuperarContraseñaController">
   <children>
      <Label layoutX="171.0" layoutY="36.0" text="Recuperar Contraseña">
         <font>
            <Font name="Arial Bold" size="24.0" />
         </font>
      </Label>
      <Label layoutX="148.0" layoutY="112.0" text="Email">
         <font>
            <Font name="Arial Italic" size="18.0" />
         </font>
      </Label>
      <Label layoutX="136.0" layoutY="209.0" text="Código">
         <font>
            <Font name="Arial Italic" size="18.0" />
         </font>
      </Label>
      <Label layoutX="43.0" layoutY="269.0" text="Nueva Contraseña">
         <font>
            <Font name="Arial Italic" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtEmail" layoutX="226.0" layoutY="110.0" prefHeight="25.0" prefWidth="285.0" promptText="ingresa el Email para generar el Código" />
      <TextField fx:id="txtCodigo" layoutX="226.0" layoutY="207.0" prefHeight="25.0" prefWidth="285.0" />
      <PasswordField fx:id="txtPassword" layoutX="226.0" layoutY="267.0" prefHeight="25.0" prefWidth="285.0" />
      <Button fx:id="btnCambiarContraseña" layoutX="82.0" layoutY="328.0" mnemonicParsing="false" onAction="#eventoCambiarContraseña" text="Cambiar Contraseña">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="btnCancelar" layoutX="440.0" layoutY="328.0" mnemonicParsing="false" onAction="#eventoCancelar" text="Cancelar">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="btnGenerarCodigo" layoutX="311.0" layoutY="151.0" mnemonicParsing="false" onAction="#eventoGenerarCodigo" text="Generar Código">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
